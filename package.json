{
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development.local next dev",
    "lint": "next lint",
    "build": "next build",
    "build:test": "export NODE_ENV=test && dotenv -e .env.test.local next build",
    "start": "next start",
    "start:test": "export NODE_ENV=test && dotenv -e .env.test.local next start",
    "test": "dotenv -e .env.test.local jest --watch",
    "test:ci": "dotenv -e .env.test.local jest --ci",
    "cypress:open": "dotenv -e .env.test.local cypress open",
    "cypress:start": "start-server-and-test 'pnpm run start:test' 3000 'pnpm run cypress:open'",
    "cypress:build": "pnpm run build:test && pnpm run cypress:start",
    "cypress:run": "start-server-and-test 'pnpm run start:test' 3000 'dotenv -e .env.test.local cypress run'"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
