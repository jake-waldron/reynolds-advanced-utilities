    "eslint-plugin-testing-library": "^5.10.2",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "prisma": "^4.11.0",
