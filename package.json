    "@supabase/auth-ui-shared": "^0.1.3",
    "@supabase/supabase-js": "^2.12.0",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "^18.15.3",
    "@types/react": "18.0.28",
